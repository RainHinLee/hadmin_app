#history_alter
	max-width 760px
	width 760px
	h3
		position relative
		background #DFBEDF
		color #fff
		font-size 20px
		border-radius 0px
		span
			position absolute
			width 20px
			height 20px
			right 5px
			top 5px
			font-size 16px
			cursor pointer
			&:hover
				color #ED1C24
	.body
		th
			color #444
			font-weight normal
			text-align center
			font-size 14px
		td
			color #666
			text-align center
			font-size 14px
			
			.btn
				padding 4px 12px
				font-size 13px
				&.btn-default
					background #F49BB9
					border 1px solid #F57FA6 
					color #fff
			&:last-child
				width 22%
				color #ED1C24