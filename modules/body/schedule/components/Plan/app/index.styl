.schedule
	&.planBox
		.searchBox
			select
			input
				width 130px
				height 30px
				border 1px solid #ddd
				border-radius 3px
				color #444
				margin-right 5px
				margin-left 5px
				outline none
				padding-left 5px
				&:focus
					border 1px solid #11a84e
		.resultBox
			padding-top 10px
			th
			td
				text-align center
				&.empty
					height 300px
					line-height 200px
				select
			th
				background #fa787d
				color #fff
			tr
				td:nth-last-of-type(1)
					span
						color #fa787d
						text-decoration underline
						cursor pointer
						&:hover
							color red
				&:hover
					td
						background #FAD6D7
						
			
			