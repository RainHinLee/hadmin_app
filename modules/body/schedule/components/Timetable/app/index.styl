.schedule
	&.timetableBox
		.searchBox
			position relative
			z-index 2
			>p	
				select
				input
					width 130px
					height 30px
					border 1px solid #ddd
					border-radius 3px
					color #444
					margin-right 5px
					margin-left 5px
					outline none
					padding-left 5px
					&:focus
						border 1px solid #11a84e
				
				*
					vertical-align middle
			._hselect
				input
					border 1px solid #ddd
					border-radius 0px	
					margin 0px			
		.resultBox
			.calendar
				.panel
					margin-bottom 0px
				.panel-heading
					padding 10px 0px
					font-size 13px
				.prev
				.next
					cursor pointer
					&:hover
						color #FA787D
				.panel-body
					padding 0px 
					height 285px
					td
						font-size 13px
						text-align center
						position relative
						left 0px
						top 0px
						span.busy
							position absolute
							right 2px
							top 3px
							width 18px
							height 18px
							border-radius 50%
							background-color #8BD8A2
							font-size 12px
							text-align center
							line-height 17px
							color #fff
							transform scale(0.95)
							
						&:hover
							cursor pointer
							background #FA787D
							color #fff
						&.disable
						&.expire
							color #ccc
							span.busy
								background-color #ccc
							&:hover	
							&.active
								background-color #ccc
								color #fff
								span.busy
									background-color #F6D5E0
						&.active
							background-color #FA787D
							color #fff
						&.selected
							background-color #C8BFE7
							color #fff
							
			.calendarTable				
				height 300px
				padding-left 0px
				.panel
					margin-bottom 0px
				.panel-heading
					height 40px
					line-height 40px
					padding 0px
				.panel-body
					height 284px
					padding 0px
					overflow-y auto
					tr
						th
						td
							text-align center
							span
								color #fa787d
								text-decoration underline
								cursor pointer
								&:hover
									color red		
						td.empty
							height 200px
							line-height 200px
						&.offline
							td
								color #fa787d					
					
						
								