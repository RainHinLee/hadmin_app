.course_show
	margin-top 20px
	h3
		font-size 14px
		text-align center
		font-weight normal
		margin 0px
		padding 5px 0px
	table
		tr
			th
			td
				font-size 14px
				font-weight normal
				color #444
				text-align center
				span
					color #22B14C
					text-decoration underline
					cursor pointer
					&:hover
						color #E6494F
			th
				background #fa787d
				color #fff
				
			&.empty
				td
					height 250px
					line-height 250px
		